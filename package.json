{
  "name": "WAMS",
  "private": true,
  "packageManager": "pnpm@9.12.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "clean": "turbo run clean",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "cross-env NODE_OPTIONS=--max-old-space-size=4096 tsc --noEmit",
    "build:docker-dev": "turbo run build:docker-dev",
    "build:docker-prod": "turbo run build:docker-prod",
    "build:docker-qa": "turbo run build:docker-qa",
    "build:dev-local": "turbo run build:dev-local",
    "build:dev-be-local": "turbo run build:dev-be-local",
    "build:development": "turbo run build:development",
    "start:docker-dev": "turbo run start:docker-dev",
    "start:docker-prod": "turbo run start:docker-prod",
    "start:docker-qa": "turbo run start:docker-qa",
    "start:dev-be-local": "turbo run start:dev-be-local",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint",
      "tsc --pretty --noEmit --skipLibCheck --jsx preserve --resolveJsonModule --esModuleInterop"
    ],
    "*.{json,js,ts,jsx,tsx,html}": [
      "prettier --write --ignore-unknown"
    ]
  },
